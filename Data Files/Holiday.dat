   <query>SELECT 
CASE WHEN (SELECT COUNT(*) AS CheckHoliday FROM ( 
				SELECT CONVERT(VARCHAR,HolidayDate,103) AS HolidayDate FROM aab.dbo.Holiday A 
				WHERE A.ActiveStatusID = 1 
				AND A.RowStatus = 0 
				AND (SELECT CONVERT(VARCHAR,HolidayDate,103)) = (SELECT CONVERT(varchar,GETDATE()+1,103)) 
				AND (SELECT DATEPART(DW,GETDATE()+1)) IN (2,3,4,5,6) 
				) AS HOLIDAY 
			) = 0 
			THEN CONVERT(varchar,GETDATE()+1,103) 
	WHEN (SELECT COUNT(*) AS CheckHoliday FROM ( 
				SELECT CONVERT(VARCHAR,HolidayDate,103) AS HolidayDate FROM aab.dbo.Holiday A 
				WHERE A.ActiveStatusID = 1 
				AND A.RowStatus = 0 
				AND (SELECT CONVERT(VARCHAR,HolidayDate,103)) = (SELECT CONVERT(varchar,GETDATE()+2,103)) 
				AND (SELECT DATEPART(DW,GETDATE()+1)) IN (2,3,4,5,6) 
				) AS HOLIDAY 
			) = 0 
			THEN CONVERT(varchar,GETDATE()+2,103) 
	WHEN (SELECT COUNT(*) AS CheckHoliday FROM ( 
				SELECT CONVERT(VARCHAR,HolidayDate,103) AS HolidayDate FROM aab.dbo.Holiday A 
				WHERE A.ActiveStatusID = 1 
				AND A.RowStatus = 0 
				AND (SELECT CONVERT(VARCHAR,HolidayDate,103)) = (SELECT CONVERT(varchar,GETDATE()+3,103)) 
				AND (SELECT DATEPART(DW,GETDATE()+1)) IN (2,3,4,5,6) 
				) AS HOLIDAY 
			) = 0 
			THEN CONVERT(varchar,GETDATE()+3,103) 
END AS SurveySchedule;</query>
